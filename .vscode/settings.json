{
  "prettier.enable": false,
  "eslint.quiet": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "search.exclude": {
    "**/.umi": true,
    "**/.git": true,
    "**/.github": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true,
    "**/.history/**": true,
    "**/README.md": true,
    "**/LICENSE": true
  },
  "cSpell.words": [
    "antfu",
    "bumpp",
    "ependencies",
    "eqeqeq",
    "IIFE",
    "jsdelivr",
    "pnpm",
    "stroustrup",
    "vitepress"
  ]
}
